<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/play_theme"
    android:layout_width="match_parent"
    android:background="#FFFFFF"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="* 소분류 (택 1)"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:id="@+id/little_type_text_play"/>

    <Button
        android:layout_below="@+id/little_type_text_play"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:id="@+id/billiard_button"
        android:background="@drawable/button_round"
        android:text="#당구장"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/little_type_text_play"

        android:id="@+id/pc_room_button"
        android:layout_toRightOf="@+id/billiard_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#피시방"
        />
    <Button
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_below="@+id/little_type_text_play"

        android:id="@+id/singing_room"
        android:layout_toRightOf="@+id/pc_room_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#노래방"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/billiard_button"
        android:id="@+id/escape_room_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round"
        android:textColor="@drawable/text_selector"

        android:text="#방탈출"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/billiard_button"
        android:id="@+id/bowling_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_toRightOf="@+id/escape_room_button"
        android:textColor="@drawable/text_selector"

        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#볼링장"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bowling_button"
        android:text="* 테마 (택 2)"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:id="@+id/theme_text_resturant"
        />

    <Button
        android:layout_below="@id/theme_text_resturant"
        android:layout_marginLeft="20dp"
        android:textColor="@drawable/text_selector"
        android:layout_marginTop="20dp"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:id="@+id/delicious_button"
        android:background="@drawable/button_round"
        android:text="#존맛"
        />
    <Button
        android:layout_below="@id/theme_text_resturant"
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"

        android:id="@+id/chip_button"
        android:layout_toRightOf="@+id/delicious_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#저렴"
        />
    <Button
        android:layout_below="@id/theme_text_resturant"
        android:layout_marginTop="20dp"
        android:textColor="@drawable/text_selector"

        android:id="@+id/clean_button"
        android:layout_toRightOf="@+id/chip_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#깨끗"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/delicious_button"
        android:id="@+id/kind_button"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round"
        android:textColor="@drawable/text_selector"

        android:text="#친절"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/delicious_button"
        android:id="@+id/background_button_resturant"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_toRightOf="@+id/kind_button"
        android:textColor="@drawable/text_selector"

        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#전망"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/delicious_button"
        android:id="@+id/different_button_resturant"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:textColor="@drawable/text_selector"
        android:layout_toRightOf="@+id/background_button_resturant"
        android:layout_marginLeft="10dp"
        android:background="@drawable/button_round"
        android:text="#이색"
        />
    <Button
        android:layout_marginTop="10dp"
        android:layout_below="@+id/kind_button"
        android:id="@+id/many_button"
        android:textColor="@drawable/text_selector"

        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_marginLeft="20dp"
        android:background="@drawable/button_round"
        android:text="#푸짐">

    </Button>
</RelativeLayout>